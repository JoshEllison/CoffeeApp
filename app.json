{
  "expo": {
    "name": "Fortune Coffee",
    "slug": "FortuneCoffee",
    "version": "1.0.12",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.redko.fortunecoffee",
      "loadJSInBackgroundExperimental":true
    },
    "android": {
      "package": "com.redko.fortune_coffee",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
